{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": false,
    "forceConsistentCasingInFileNames": true
  }
}